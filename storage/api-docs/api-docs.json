{
    "openapi": "3.0.0",
    "info": {
        "title": "Laravel Swagger API documentation example",
        "contact": {
            "email": "admin@example.com"
        },
        "license": {
            "name": "Apache 2.0",
            "url": "http://www.apache.org/licenses/LICENSE-2.0.html"
        },
        "version": "1.0.0"
    },
    "servers": [
        {
            "url": "http://localhost/api",
            "description": "Laravel Swagger API server"
        }
    ],
    "paths": {
        "/produk": {
            "get": {
                "tags": [
                    "Produk"
                ],
                "summary": "Get a list of produk",
                "description": "Returns list of projects",
                "operationId": "getProdukList",
                "responses": {
                    "200": {
                        "description": "Everything is fine"
                    }
                }
            }
        },
        "/produk/{id}": {
            "get": {
                "tags": [
                    "Produk"
                ],
                "summary": "Get produk by ID",
                "description": "Returns project data",
                "operationId": "getProdukById",
                "parameters": [
                    {
                        "name": "id",
                        "in": "path",
                        "description": "The ID of produk",
                        "required": true,
                        "schema": {
                            "type": "integer"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Everything is fine",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "$ref": "App\\Models\\Produk"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "components": {
        "securitySchemes": {
            "X-APP-ID": {
                "type": "apiKey",
                "name": "X-APP-ID",
                "in": "header"
            }
        }
    },
    "tags": [
        {
            "name": "Examples",
            "description": "Some example pages"
        }
    ]
}